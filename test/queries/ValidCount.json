{
    "title": "Simple title query",
    "query": {
        "WHERE": {
            "EQ": {
                "courses_avg": 95
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_avg",
                "theAmount"
            ]
        },
        "TRANSFORMATIONS": {
            "GROUP": ["courses_avg"],
            "APPLY": [{
                "theAmount": {
                    "COUNT": "courses_id"
                }
            }]
        }
    },
    "isQueryValid": true,
    "result": [{"courses_avg":95,"theAmount":17}]
}
