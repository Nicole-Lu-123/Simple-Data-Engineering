{
    "title": "APPLY contain courses but others contain room",
    "query": 
{
    "WHERE": {
        "AND": [{
            "IS": {
                "rooms_furniture": "*Tables*"
            }
        }, {
            "GT": {
                "rooms_seats": 300
            }
        }]
    },
    "OPTIONS": {
        "COLUMNS": [
            "rooms_shortname",
            "avgcourse"
        ],
        "ORDER": {
            "dir": "DOWN",
            "keys": ["maxSeats"]
        }
    },
    "TRANSFORMATIONS": {
        "GROUP": ["rooms_shortname"],
        "APPLY": [{
            "avgcourse": {
                "AVG": "course_avg"
            }
        }]
    }
},
    "isQueryValid": false,
    "result": "InsightError"
}
