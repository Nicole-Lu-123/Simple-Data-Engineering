{
    "title": "D2 simple course max query",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_dept": "c*"
                    }
                },
                {
                    "GT": {
                        "courses_avg": 90
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "maxAvg"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "maxAvg",
                    "courses_dept"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_dept"
            ],
            "APPLY": [
                {
                    "maxAvg": {
                        "MAX": "courses_avg"
                    }
                }
            ]
        }
    },
