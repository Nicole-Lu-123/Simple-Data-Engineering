{
    "title": "D2 course query multiple apply",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_dept": "c*"
                    }
                },
                {
                    "GT": {
                        "courses_avg": 90
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "countDept",
                "sumAvg"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "countDept",
                    "courses_dept",
                    "sumAvg"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_dept"
            ],
            "APPLY": [
                {
                    "countDept": {
                        "COUNT": "courses_avg"
                    }
                },
                {
                    "sumAvg": {
                        "SUM": "courses_avg"
                    }
                }
            ]
        }
    },
