{
    "title": "Query containing Section value overall, year should be set to 1900",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "GT": {
                        "courses_avg": 99
                    }
                },
                {
                    "EQ": {
                        "courses_year": 1900
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "courses_id",
                "courses_avg",
                "courses_uuid",
                "courses_year"
            ],
            "ORDER": "courses_avg"
        }
    },
    "isQueryValid": true,
    "result": [
        {
            "courses_dept": "math",
            "courses_id": "527",
            "courses_avg": 99.78,
            "courses_uuid": "5374",
            "courses_year": 1900
        }
    ]
}
