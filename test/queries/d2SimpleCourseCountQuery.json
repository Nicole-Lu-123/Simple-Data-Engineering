{
    "title": "D2 simple course count query",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_dept": "c*"
                    }
                },
                {
                    "GT": {
                        "courses_avg": 90
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "countAvg"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "countAvg",
                    "courses_dept"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_dept"
            ],
            "APPLY": [
                {
                    "countAvg": {
                        "COUNT": "courses_avg"
                    }
                }
            ]
        }
    },
