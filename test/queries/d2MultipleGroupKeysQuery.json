{
    "title": "D2 multiple group keys query",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 80
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
