{
    "title": "D2 simple course avg query",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_dept": "c*"
                    }
                },
                {
                    "GT": {
                        "courses_avg": 90
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "avgAvg"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "avgAvg",
                    "courses_dept"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_dept"
            ],
            "APPLY": [
                {
                    "avgAvg": {
                        "AVG": "courses_avg"
                    }
                }
            ]
        }
    },
