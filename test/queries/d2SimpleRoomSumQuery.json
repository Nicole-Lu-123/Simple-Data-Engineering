{
    "title": "D2 simple room sum query",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 80
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "sumSeats",
                "rooms_shortname"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "sumSeats",
                    "rooms_shortname"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_shortname"
            ],
            "APPLY": [
                {
                    "sumSeats": {
                        "SUM": "rooms_seats"
                    }
                }
            ]
        }
    },
