{
    "title": "D2 simple course min query",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "courses_dept": "c*"
                    }
                },
                {
                    "GT": {
                        "courses_avg": 90
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_dept",
                "minAvg"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "minAvg",
                    "courses_dept"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_dept"
            ],
            "APPLY": [
                {
                    "minAvg": {
                        "MIN": "courses_avg"
                    }
                }
            ]
        }
    },
