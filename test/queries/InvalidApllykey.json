{
    "title": "Simple uuid query",
    "query": {
        "WHERE": {
            "IS": {
                "courses_uuid": "70099"
            }
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_uuid",
                "_overallavg"
            ],
            "TRANSFORMATIONS": {
                "GROUP": ["courses_title"],
                "APPLY": [{
                    "overallAvg": {
                        "AVG": "courses_avg"
                    }
                }]
            }
        }
    },
    "isQueryValid": false,
    "result": "InsightError"
}
