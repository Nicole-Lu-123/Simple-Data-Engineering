{
    "title": "D2 query different room dataset",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "roomsDifferentId_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "roomsDifferentId_seats": 200
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "sumSeats",
                "roomsDifferentId_number"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "sumSeats",
                    "roomsDifferentId_number"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "roomsDifferentId_number"
            ],
            "APPLY": [
                {
                    "sumSeats": {
                        "SUM": "roomsDifferentId_seats"
                    }
                }
            ]
        }
    },
